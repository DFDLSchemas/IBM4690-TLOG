	* Version 1.0 	TlogAce.xsd
	*
	* This DFDL schema is supplied by IBM. It provides a DFDL model for
	* 4690 point-of-sale transaction log (TLog) data (ACE format).
	*
	* Each Transaction consists of a number of TransactionRecords.
	* Each TransactionRecord is of a particular type given by the StringType element.
	* Initiators are used to discriminate the record types.
	* Some record types have sub-types which are discriminated either using
	* initiators or discriminators.
	* 
	******************************************************************* --><xsd:schema xmlns:dfdl="http://www.ogf.org/dfdl/dfdl-1.0/" xmlns:fmt="http://www.ibm.com/dfdl/TlogAceFormat" xmlns:ibmDfdlExtn="http://www.ibm.com/dfdl/extensions" xmlns:ibmSchExtn="http://www.ibm.com/schema/extensions" xmlns:xsd="http://www.w3.org/2001/XMLSchema">

        
  <xsd:include schemaLocation="TlogAceTypes.xsd"/>
  <xsd:import namespace="http://www.ibm.com/dfdl/TlogAceFormat" schemaLocation="TlogAceFormat.xsd"/>
  <xsd:include schemaLocation="TlogAceCommon.xsd"/>
  <xsd:include schemaLocation="TlogAceCustom.xsd"/>

  <xsd:element ibmSchExtn:docRoot="true" name="Transactions">
    <xsd:complexType>
      <xsd:sequence dfdl:separator="">
        <xsd:element maxOccurs="unbounded" ref="Transaction"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>

    <xsd:annotation>
		<xsd:appinfo source="http://www.ogf.org/dfdl/">
			<dfdl:format ref="fmt:TlogAceFormat"/>
		</xsd:appinfo>
	</xsd:annotation>

    <xsd:element dfdl:emptyValueDelimiterPolicy="both" dfdl:initiator="&quot;" dfdl:terminator="&quot;%CR;%LF;" ibmSchExtn:docRoot="true" name="Transaction">
        <xsd:complexType>
            <xsd:sequence dfdl:separator="&quot;,&quot;" dfdl:separatorPolicy="suppressed">
                <xsd:element maxOccurs="unbounded" name="TransactionRecord">
                                        <xsd:annotation>
            <xsd:appinfo source="http://www.ogf.org/dfdl/"/>
          </xsd:annotation>
                    <xsd:complexType>
                    	<!-- -->
                    	<xsd:choice>
                    		<!-- All transaction categories are distinguished by a unique initiator -->
							<!-- All branches of the following choice must have 2 byte initiators of the form '%#xNN;:' -->
                            <xsd:choice dfdl:initiatedContent="yes">
    	                        <xsd:element ref="TransactionRecord00">
					            </xsd:element>
	                            <xsd:element ref="TransactionRecord01">
					            </xsd:element>
    	                        <xsd:element ref="TransactionRecord02">
					            </xsd:element>
                                <xsd:element ref="TransactionRecord03">
					            </xsd:element>
                                <xsd:element ref="TransactionRecord04">
					            </xsd:element>
                                <xsd:element ref="TransactionRecord05">
					            </xsd:element>
                                <xsd:element ref="TransactionRecord06">
					            </xsd:element>
                                <xsd:element ref="TransactionRecord07">
					            </xsd:element>
                                <xsd:element ref="TransactionRecord08">
					            </xsd:element>
                                <xsd:element ref="TransactionRecord09">
					            </xsd:element>
                                <xsd:element ref="TransactionRecord10">
					            </xsd:element>
                                <xsd:group ref="choice_11">
                                </xsd:group>
                                <xsd:element ref="TransactionRecord13">
                                </xsd:element>
                            	<xsd:element ref="TransactionRecord16">
					            </xsd:element>
                            	<xsd:element ref="TransactionRecord21">
					            </xsd:element>
                                <xsd:group ref="choice_80">
					            </xsd:group>
                                <xsd:group ref="choice_97">
					            </xsd:group>
                                <xsd:group ref="choice_99">
					            </xsd:group>
					            <!-- 20 are exception log records so put at end -->
                            	<xsd:group ref="choice_20">
					            </xsd:group>
					        </xsd:choice>
							<!-- 98 has 1 byte initiator -->
                            <xsd:element ref="TransactionRecord98">
				            </xsd:element>
                           	<!-- Custom group -->
                            <xsd:group ref="choice_Custom">
				            </xsd:group>
                            <xsd:element ref="TransactionRecordXX">
					        </xsd:element>
                        </xsd:choice>
                    </xsd:complexType>
                </xsd:element>
             </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

	<!-- Catch-all record so that everything can always be parsed -->
    <xsd:element name="TransactionRecordXX">
        <xsd:complexType>
            <xsd:sequence>
           		<xsd:element dfdl:length="1" dfdl:lengthKind="explicit" name="StringType" type="type_PS"/>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="CustomUserField"/>
            </xsd:sequence>
		</xsd:complexType>
    </xsd:element>
    
    <xsd:element dfdl:emptyValueDelimiterPolicy="initiator" dfdl:encoding="ISO-8859-1" dfdl:initiator="%#x00;:" name="TransactionRecord00">
        <xsd:complexType>
            <xsd:sequence>
           		<!--
           		<xsd:element dfdl:inputValueCalc="{xs:hexBinary('00')}" name="StringType" type="type_PS">
           		</xsd:element> 
           		-->
                <xsd:element dfdl:length="2" dfdl:lengthKind="explicit" name="TerminalNumber" type="type_PS">
                </xsd:element>
                <xsd:element dfdl:length="2" dfdl:lengthKind="explicit" ibmDfdlExtn:sampleValue="1" name="TransactionNumber" type="type_PI">
                </xsd:element>
                <xsd:element dfdl:length="5" dfdl:lengthKind="explicit" name="DateTime" type="type_PI">
                </xsd:element>
                <xsd:element dfdl:length="1" dfdl:lengthKind="explicit" ibmDfdlExtn:sampleValue="0F00" name="TransactionType" type="type_PS">
                </xsd:element>
                <xsd:element dfdl:length="2" dfdl:lengthKind="explicit" name="NumberOfStrings" type="type_PD">
                </xsd:element>
                <xsd:element   ibmDfdlExtn:sampleValue="1.0" minOccurs="0" name="Operator" type="type_PD">
                </xsd:element>
                <xsd:element   minOccurs="0" name="Password" type="type_PD">
                </xsd:element>
                <xsd:element   minOccurs="0" name="GrossPositive" type="type_PL">
                </xsd:element>
                <xsd:element   minOccurs="0" name="GrossNegative" type="type_PL">
                </xsd:element>
                <xsd:element   minOccurs="0" name="RingTime" type="type_PL">
                </xsd:element>
                <xsd:element   minOccurs="0" name="TenderTime" type="type_PL">
                </xsd:element>
                <xsd:element   minOccurs="0" name="SpecialTime" type="type_PL">
                </xsd:element>
                <xsd:element   minOccurs="0" name="InactiveTime" type="type_PL">
                </xsd:element>
                <xsd:element   minOccurs="0" name="Indicat1" type="type_4ByteBitField_Desc">
				</xsd:element>
                <xsd:element   minOccurs="0" name="AltPrice" type="type_PD">
				</xsd:element>
                <xsd:element   minOccurs="0" name="VoidTrc" type="type_PD">
				</xsd:element>
				<xsd:element ibmDfdlExtn:sampleValue="0F00" maxOccurs="unbounded" minOccurs="0" ref="CustomUserField"/>
            </xsd:sequence>
		</xsd:complexType>
    </xsd:element>

    <xsd:element dfdl:emptyValueDelimiterPolicy="initiator" dfdl:encoding="ISO-8859-1" dfdl:initiator="%#x01;:" name="TransactionRecord01">
	    <xsd:complexType>
            <xsd:sequence>
           		<!--
           		<xsd:element dfdl:inputValueCalc="{xs:hexBinary('01')}" name="StringType" type="type_PS"> 
           		-->
                <xsd:element   minOccurs="0" name="ItemCode" type="type_PD">
                </xsd:element>
                <xsd:element   minOccurs="0" name="ExtendedPrice" type="type_PD">
                </xsd:element>
                <xsd:element   minOccurs="0" name="DepartmentNumber" type="type_PD">
                </xsd:element>
                <xsd:element   minOccurs="0" name="CouponFamilyNumber" type="type_PD">
                </xsd:element>
                <xsd:element   minOccurs="0" name="Indicat1" type="type_4ByteBitField_Desc">
                </xsd:element>
                <xsd:element   minOccurs="0" name="Indicat2" type="type_2ByteBitField_ByteReversedLittleIndian">                                 
                </xsd:element>
                <xsd:element dfdl:occursCountKind="implicit" maxOccurs="1" minOccurs="0" name="Indicat3" type="type_PS">
                </xsd:element>
                <xsd:element   minOccurs="0" name="Indicat4" type="type_4ByteBitField">
                </xsd:element>
                <xsd:element   minOccurs="0" name="OrdinalNumber" type="type_PI">
				</xsd:element>
				<xsd:element maxOccurs="unbounded" minOccurs="0" ref="CustomUserField"/>
            </xsd:sequence>
		</xsd:complexType>
   	</xsd:element>

    <xsd:element dfdl:emptyValueDelimiterPolicy="initiator" dfdl:encoding="ISO-8859-1" dfdl:initiator="%#x02;:" name="TransactionRecord02">
	    <xsd:complexType>
            <xsd:sequence>
           		<!--
           		<xsd:element dfdl:inputValueCalc="{xs:hexBinary('02')}" name="StringType" type="type_PS"> 
           		-->
                <xsd:element   minOccurs="0" name="MultiPricingGroup" type="type_PD">
                </xsd:element>
                <xsd:element   minOccurs="0" name="DealQuantity" type="type_PD">
                </xsd:element>
                <xsd:element   minOccurs="0" name="PricingMethod" type="type_PD">
                </xsd:element>
                <xsd:element   minOccurs="0" name="SaleQuantity" type="type_PD">
                </xsd:element>
                <xsd:element   minOccurs="0" name="SalePrice" type="type_PD">
                </xsd:element>
                <xsd:element   minOccurs="0" name="QtyOrWgtOrVol" type="type_PD">
                </xsd:element>
                <xsd:element   minOccurs="0" name="Indicat1" type="type_1ByteBitField">
                </xsd:element>
				<xsd:element maxOccurs="unbounded" minOccurs="0" ref="CustomUserField"/>
            </xsd:sequence>
		</xsd:complexType>
    </xsd:element>

    <xsd:element dfdl:emptyValueDelimiterPolicy="initiator" dfdl:encoding="ISO-8859-1" dfdl:initiator="%#x03;:" name="TransactionRecord03">
        <xsd:complexType>
        	<xsd:sequence>
           		<!--
           		<xsd:element dfdl:inputValueCalc="{xs:hexBinary('03')}" name="StringType" type="type_PS"> 
           		-->
            	<xsd:group ref="sequence_03_and_04">
            	</xsd:group>
            </xsd:sequence>
		</xsd:complexType>
    </xsd:element>

    <xsd:element dfdl:emptyValueDelimiterPolicy="initiator" dfdl:encoding="ISO-8859-1" dfdl:initiator="%#x04;:" name="TransactionRecord04">
        <xsd:complexType>
        	<xsd:sequence>
           		<!--
           		<xsd:element dfdl:inputValueCalc="{xs:hexBinary('04')}" name="StringType" type="type_PS"> 
           		-->
    	        <xsd:group ref="sequence_03_and_04">
	            </xsd:group>
            </xsd:sequence>
		</xsd:complexType>
    </xsd:element>

	<xsd:group name="sequence_03_and_04">
		<xsd:sequence>
                <xsd:element   minOccurs="0" name="DiscountGroup" type="type_PI">
                </xsd:element>
                <xsd:element   minOccurs="0" name="DiscountRate" type="type_PI">
                </xsd:element>
                <xsd:element   minOccurs="0" name="DiscountAmount" type="type_PL">
                </xsd:element>
                <xsd:element   minOccurs="0" name="DiscountTaxExemptAmount" type="type_PL">
                </xsd:element>
                <xsd:element   minOccurs="0" name="TaxableExemptAmount" type="type_PL">
                </xsd:element>
                <xsd:element   minOccurs="0" name="TaxExemptId" type="type_ASCII">
                </xsd:element>
                <xsd:element   minOccurs="0" name="TaxExemptAmountA" type="type_PL">
                </xsd:element>
                <xsd:element   minOccurs="0" name="TaxExemptAmountB" type="type_PL">
                </xsd:element>
                <xsd:element   minOccurs="0" name="TaxExemptAmountC" type="type_PL">
                </xsd:element>
                <xsd:element   minOccurs="0" name="TaxExemptAmountD" type="type_PL">
                </xsd:element>
                <xsd:element   minOccurs="0" name="TaxableExemptAmountA" type="type_PL">
                </xsd:element>
                <xsd:element   minOccurs="0" name="TaxableExemptAmountB" type="type_PL">
                </xsd:element>
                <xsd:element   minOccurs="0" name="TaxableExemptAmountC" type="type_PL">
                </xsd:element>
                <xsd:element   minOccurs="0" name="TaxableExemptAmountD" type="type_PL">
                </xsd:element>
                <xsd:element   minOccurs="0" name="TaxExemptAmountE" type="type_PL">
                </xsd:element>
                <xsd:element   minOccurs="0" name="TaxExemptAmountF" type="type_PL">
                </xsd:element>
                <xsd:element   minOccurs="0" name="TaxExemptAmountG" type="type_PL">
                </xsd:element>
                <xsd:element   minOccurs="0" name="TaxExemptAmountH" type="type_PL">
                </xsd:element>
                <xsd:element   minOccurs="0" name="TaxableExemptAmountE" type="type_PL">
                </xsd:element>
                <xsd:element   minOccurs="0" name="TaxableExemptAmountF" type="type_PL">
                </xsd:element>
                <xsd:element   minOccurs="0" name="TaxableExemptAmountG" type="type_PL">
                </xsd:element>
                <xsd:element   minOccurs="0" name="TaxableExemptAmountH" type="type_PL">
                </xsd:element>
				<xsd:element maxOccurs="unbounded" minOccurs="0" ref="CustomUserField"/>
		</xsd:sequence>
	</xsd:group>
	  
    <xsd:element dfdl:emptyValueDelimiterPolicy="initiator" dfdl:encoding="ISO-8859-1" dfdl:initiator="%#x05;:" name="TransactionRecord05">
        <xsd:complexType>
        	<xsd:sequence>
           		<!--
           		<xsd:element dfdl:inputValueCalc="{xs:hexBinary('05')}" name="StringType" type="type_PS">
           		</xsd:element> 
           		-->
	            <xsd:group ref="sequence_05_and_06">
    	        </xsd:group>
            </xsd:sequence>
		</xsd:complexType>
    </xsd:element>

    <xsd:element dfdl:emptyValueDelimiterPolicy="initiator" dfdl:encoding="ISO-8859-1" dfdl:initiator="%#x06;:" name="TransactionRecord06">
        <xsd:complexType>
        	<xsd:sequence>
           		<!--
           		<xsd:element dfdl:inputValueCalc="{xs:hexBinary('06')}" name="StringType" type="type_PS">
           		</xsd:element> 
           		-->
	            <xsd:group ref="sequence_05_and_06">
    	        </xsd:group>
            </xsd:sequence>
		</xsd:complexType>
    </xsd:element>

	<xsd:group name="sequence_05_and_06">
		<xsd:sequence>
                <xsd:element   minOccurs="0" name="TenderType" type="type_PI">
                </xsd:element>
                <xsd:element   minOccurs="0" name="TenderAmount" type="type_PL">
                </xsd:element>
                <xsd:element   minOccurs="0" name="TenderCashingFee" type="type_PL">
                </xsd:element>
                <xsd:element   minOccurs="0" name="Customer" type="type_PD">
                </xsd:element>
                <xsd:element   minOccurs="0" name="Status" type="type_PI">
                </xsd:element>
				<xsd:element maxOccurs="unbounded" minOccurs="0" ref="CustomUserField"/>
		</xsd:sequence>
	</xsd:group>

    <xsd:element dfdl:emptyValueDelimiterPolicy="initiator" dfdl:encoding="ISO-8859-1" dfdl:initiator="%#x07;:" name="TransactionRecord07">
        <xsd:complexType>
        	<xsd:sequence>
           		<!--
           		<xsd:element dfdl:inputValueCalc="{xs:hexBinary('07')}" name="StringType" type="type_PS">
           		</xsd:element> 
           		-->
	            <xsd:group ref="sequence_07_and_08">
    	        </xsd:group>
            </xsd:sequence>
		</xsd:complexType>
    </xsd:element>

    <xsd:element dfdl:emptyValueDelimiterPolicy="initiator" dfdl:encoding="ISO-8859-1" dfdl:initiator="%#x08;:" name="TransactionRecord08">
        <xsd:complexType>
        	<xsd:sequence>
           		<!--
           		<xsd:element dfdl:inputValueCalc="{xs:hexBinary('08')}" name="StringType" type="type_PS">
           		</xsd:element> 
           		-->
            	<xsd:group ref="sequence_07_and_08">
            	</xsd:group>
            </xsd:sequence>
		</xsd:complexType>
    </xsd:element>

	<xsd:group name="sequence_07_and_08">
		<xsd:sequence>
                <xsd:element   minOccurs="0" name="TaxAAmount" type="type_PL">
                </xsd:element>
                <xsd:element   minOccurs="0" name="TaxBAmount" type="type_PL">
                </xsd:element>
                <xsd:element   minOccurs="0" name="TaxCAmount" type="type_PL">
                </xsd:element>
                <xsd:element   minOccurs="0" name="TaxDAmount" type="type_PL">
                </xsd:element>
                <xsd:element   minOccurs="0" name="TaxASales" type="type_PL">
                </xsd:element>
                <xsd:element   minOccurs="0" name="TaxBSales" type="type_PL">
                </xsd:element>
                <xsd:element   minOccurs="0" name="TaxCSales" type="type_PL">
                </xsd:element>
                <xsd:element   minOccurs="0" name="TaxDSales" type="type_PL">
                </xsd:element>
                <xsd:element   minOccurs="0" name="TaxEAmount" type="type_PL">
                </xsd:element>
                <xsd:element   minOccurs="0" name="TaxFAmount" type="type_PL">
                </xsd:element>
                <xsd:element   minOccurs="0" name="TaxGAmount" type="type_PL">
                </xsd:element>
                <xsd:element   minOccurs="0" name="TaxHAmount" type="type_PL">
                </xsd:element>
                <xsd:element   minOccurs="0" name="TaxESales" type="type_PL">
                </xsd:element>
                <xsd:element   minOccurs="0" name="TaxFSales" type="type_PL">
                </xsd:element>
                <xsd:element   minOccurs="0" name="TaxGSales" type="type_PL">
                </xsd:element>
                <xsd:element   minOccurs="0" name="TaxHSales" type="type_PL">
                </xsd:element>
				<xsd:element maxOccurs="unbounded" minOccurs="0" ref="CustomUserField"/>
		</xsd:sequence>
	</xsd:group>

    <xsd:element dfdl:emptyValueDelimiterPolicy="initiator" dfdl:encoding="ISO-8859-1" dfdl:initiator="%#x09;:" name="TransactionRecord09">
        <xsd:complexType>
            <xsd:sequence>
           		<!--
           		<xsd:element dfdl:inputValueCalc="{xs:hexBinary('09')}" name="StringType" type="type_PS">
           		</xsd:element> 
           		-->
                <xsd:element   minOccurs="0" name="TenderType" type="type_PI">
                </xsd:element>
                <xsd:element   minOccurs="0" name="TenderAmount" type="type_PL">
                </xsd:element>
				<xsd:element maxOccurs="unbounded" minOccurs="0" ref="CustomUserField"/>
            </xsd:sequence>
		</xsd:complexType>
    </xsd:element>

    <xsd:element dfdl:emptyValueDelimiterPolicy="initiator" dfdl:encoding="ISO-8859-1" dfdl:initiator="%#x10;:" name="TransactionRecord10">
        <xsd:complexType>
            <xsd:sequence>
           		<!--
           		<xsd:element dfdl:inputValueCalc="{xs:hexBinary('10')}" name="StringType" type="type_PS">
           		</xsd:element> 
           		-->
                <xsd:element   minOccurs="0" name="Override" type="type_PD">
                </xsd:element>
                <xsd:element   minOccurs="0" name="Reason" type="type_PS">
                </xsd:element>
                <xsd:element   minOccurs="0" name="Index" type="type_PD">
                </xsd:element>
                <xsd:element   minOccurs="0" name="Initials" type="type_ASCII">
                </xsd:element>
				<xsd:element maxOccurs="unbounded" minOccurs="0" ref="CustomUserField"/>
            </xsd:sequence>
		</xsd:complexType>
    </xsd:element>

	<!-- Group for all 11 records --> 
	<!-- All 11 records discriminate on SubStringType except the DataEntry and catch-all records -->
	<!-- When dfdl:inputValueCalc supported, we will use an initiator instead -->
	<!-- User can add custom 11 records --> 
    <xsd:group name="choice_11">
				<xsd:choice dfdl:encoding="ISO-8859-1" dfdl:initiator="%#x11;:">
					<!-- All branches of the following choice must have the same length initiator -->
					<xsd:choice>	
		                <xsd:element ref="TransactionRecord11_BB">
						</xsd:element>
    		            <xsd:element ref="TransactionRecord11_CC">
						</xsd:element>
    	        	    <xsd:element ref="TransactionRecord11_CE">
						</xsd:element>
	    	            <xsd:element ref="TransactionRecord11_DB">
						</xsd:element>
    	        	    <xsd:element ref="TransactionRecord11_DE">
						</xsd:element>
	    	            <xsd:element ref="TransactionRecord11_EE">
						</xsd:element>
    		            <xsd:element ref="TransactionRecord11_FF">
						</xsd:element>
					</xsd:choice>
					<!-- 11_DD has an extra unused field at start so breaks initiator length rule -->
   		            <xsd:element ref="TransactionRecord11_DD">
					</xsd:element>
					<!-- Custom 11 records -->
			        <xsd:group ref="choice_11_Custom">
		            </xsd:group>
    	            <xsd:element ref="TransactionRecord11_DataEntry">
					</xsd:element>
    	            <xsd:element ref="TransactionRecord11_XX">
						<xsd:annotation>
							<xsd:appinfo source="http://www.ogf.org/dfdl/">
								<dfdl:discriminator>{fn:true()}</dfdl:discriminator>
							</xsd:appinfo>
						</xsd:annotation>
					</xsd:element>
        	    </xsd:choice>
    </xsd:group>

	<!-- Data entry record for 11 -->
	<!-- Note this does not have a SubStringType field -->
    <xsd:element name="TransactionRecord11_DataEntry">
        <xsd:complexType>
            <xsd:sequence>
           		<!--
           		<xsd:element dfdl:inputValueCalc="{xs:hexBinary('11')}" name="StringType" type="type_PS">
           		</xsd:element> 
           		-->
                <xsd:element   minOccurs="0" name="UserData1" type="type_PD">
                </xsd:element>
                <xsd:element   minOccurs="0" name="UserData2" type="type_PD">
                </xsd:element>
                <xsd:element   minOccurs="0" name="UserData3" type="type_PD">
                </xsd:element>
                <xsd:element   minOccurs="0" name="UserData4" type="type_PD">
                </xsd:element>
                <xsd:element   minOccurs="0" name="UserData5" type="type_PD">
                </xsd:element>
                <xsd:element   minOccurs="0" name="UserData6" type="type_PD">
                </xsd:element>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="CustomUserField"/>
            </xsd:sequence>
		</xsd:complexType>
    </xsd:element>

	<!-- Catch-all record for 11 -->
	<!-- Note this does not have a SubStringType field -->
    <xsd:element name="TransactionRecord11_XX">
        <xsd:complexType>
            <xsd:sequence>
           		<!--
           		<xsd:element dfdl:inputValueCalc="{xs:hexBinary('11')}" name="StringType" type="type_PS">
           		</xsd:element> 
           		-->
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="CustomUserField"/>
            </xsd:sequence>
		</xsd:complexType>
    </xsd:element>

    <xsd:element name="TransactionRecord11_BB">
        <xsd:complexType>
            <xsd:sequence>
           		<!--
           		<xsd:element dfdl:inputValueCalc="{xs:hexBinary('11')}" name="StringType" type="type_PS">
           		</xsd:element> 
           		-->
           	    <xsd:element dfdl:length="1" dfdl:lengthKind="explicit" fixed="BB" name="SubStringType" type="type_PS">
						<xsd:annotation>
							<xsd:appinfo source="http://www.ogf.org/dfdl/">
