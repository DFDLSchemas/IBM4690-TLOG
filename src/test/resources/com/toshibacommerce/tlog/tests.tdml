    </tdml:document>
    <tdml:infoset>
      <tdml:dfdlInfoset type="file">com/toshibacommerce/tlog/ace_02_03_07.xml</tdml:dfdlInfoset>
    </tdml:infoset>
  </tdml:parserTestCase>
  
  <tdml:parserTestCase name="testACE3" root="Transactions" model="com/toshibacommerce/tlog/xsd/TlogAce.xsd">
