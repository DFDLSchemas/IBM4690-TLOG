  <tdml:parserTestCase name="testACE4" root="Transactions" model="com/toshibacommerce/tlog/xsd/TlogAce.xsd">
    <tdml:document>
      <tdml:documentPart type="file">com/toshibacommerce/tlog/ace_80-01_98_13_97-13.dat</tdml:documentPart>
    </tdml:document>
    <tdml:infoset>
      <tdml:dfdlInfoset type="file">com/toshibacommerce/tlog/ace_80-01_98_13_97-13.xml</tdml:dfdlInfoset>
