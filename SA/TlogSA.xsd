	*********************************************************************
	* Version 1.1 	TlogSA.xsd
	*
	* Change History
	* 1.1: Correct 80_01 AgencyId to allow for empty field
	*      Correct 80_01 Category to allow for empty field
	*      Correct 80_02 AgencyId to allow for empty field
	*      Correct 80_03 AgencyId to allow for empty field
	*      Correct 21 Indicat2 to delimited to avoid bug
	*
	********************************************************************* 
	* This DFDL schema is supplied by IBM. It provides a DFDL model for
	* 4690 point-of-sale transaction log (TLog) data (SA format).
	*
	* Each Transaction consists of a number of TransactionRecords.
	* Each TransactionRecord is of a particular type given by the StringType element.
	* Initiators are used to discriminate the record types.
	* Some record types have sub-types which are discriminated either using
	* initiators or discriminators.
    *
    * Source for this schema is the SA Programming Guide and the SA_SMKT.ini
    * file supplied by the 4690TV++ utility. The latter is taken as definitive
    * where there are differences.
	* 
	******************************************************************* --><xs:schema targetNamespace="http://www.ibm.com/dfdl/Tlog/SA" xmlns:dfdl="http://www.ogf.org/dfdl/dfdl-1.0/" xmlns:fmt="http://www.ibm.com/dfdl/TlogSAFormat" xmlns:fn="http://www.w3.org/2005/xpath-functions" xmlns:ibmDfdlExtn="http://www.ibm.com/dfdl/extensions" xmlns:ibmSchExtn="http://www.ibm.com/schema/extensions" xmlns:sa="http://www.ibm.com/dfdl/Tlog/SA" xmlns:xs="http://www.w3.org/2001/XMLSchema">

        
  <xs:include schemaLocation="TlogSATypes.xsd"/>
  <xs:import namespace="http://www.ibm.com/dfdl/TlogSAFormat" schemaLocation="TlogSAFormat.xsd"/>
  <xs:include schemaLocation="TlogSACommon.xsd"/>
  <xs:include schemaLocation="TlogSACustom.xsd"/>

    <xs:annotation>
		<xs:appinfo source="http://www.ogf.org/dfdl/">
			<dfdl:format ref="fmt:TlogSAFormat"/>
		</xs:appinfo>
	</xs:annotation>

  <xs:element ibmSchExtn:docRoot="true" name="Transactions">
    <xs:complexType>
      <xs:sequence dfdl:separator="">
        <xs:element maxOccurs="unbounded" ref="sa:Transaction"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

    <xs:element dfdl:emptyValueDelimiterPolicy="both" dfdl:initiator="&quot;" dfdl:terminator="&quot;%CR;%LF;" ibmSchExtn:docRoot="true" name="Transaction">
        <xs:complexType>
            <xs:sequence dfdl:separator="&quot;,&quot;" dfdl:separatorPolicy="suppressed">
                <xs:element maxOccurs="unbounded" name="TransactionRecord">
                    <xs:complexType>
                    	<!-- -->
                    	<xs:choice>
                    		<!-- All transaction categories are distinguished by a unique initiator -->
							<!-- All branches of the following choice must have 2 byte initiators of the form '%#xNN;:' -->
                            <xs:choice dfdl:initiatedContent="yes">
    	                        <xs:element ref="sa:TransactionRecord00">
								</xs:element>
	                        	<xs:element ref="sa:TransactionRecord01">
								</xs:element>
    	                        <xs:element ref="sa:TransactionRecord02">
								</xs:element>
                                <xs:element ref="sa:TransactionRecord03">
								</xs:element>
                                <xs:element ref="sa:TransactionRecord04">
								</xs:element>
                                <xs:element ref="sa:TransactionRecord05">
								</xs:element>
                                <xs:element ref="sa:TransactionRecord06">
								</xs:element>
                                <xs:element ref="sa:TransactionRecord07">
								</xs:element>
                                <xs:element ref="sa:TransactionRecord08">
								</xs:element>
                                <xs:element ref="sa:TransactionRecord09">
								</xs:element>
                                <xs:element ref="sa:TransactionRecord10">
								</xs:element>
                                <xs:group ref="sa:choice_11">
                                </xs:group>
                                <xs:element ref="sa:TransactionRecord12">
                                </xs:element>
                                <xs:element ref="sa:TransactionRecord13">
                                </xs:element>
                                <xs:element ref="sa:TransactionRecord14">
                                </xs:element>
                                <xs:element ref="sa:TransactionRecord15">
                                </xs:element>
                            	<xs:element ref="sa:TransactionRecord16">
                                </xs:element>
                                <xs:element ref="sa:TransactionRecord17">
                                </xs:element>
                            	<xs:element ref="sa:TransactionRecord21">
								</xs:element>
                                <xs:group ref="sa:choice_80">
					            </xs:group>
                                <xs:group ref="sa:choice_99">
								</xs:group>
								<!-- 20 are exception log records so put at end -->
                            	<xs:group ref="sa:choice_20">
								</xs:group>
			    			</xs:choice>
						    <!-- 98 has 1 byte initiator -->
                            <xs:element ref="sa:TransactionRecord98">
			    			</xs:element>
                            <!-- Custom group -->
                            <xs:group ref="sa:choice_Custom">
						    </xs:group>
                            <xs:element ref="sa:TransactionRecordXX">
						    </xs:element>
                        </xs:choice>
                    </xs:complexType>
                </xs:element>
             </xs:sequence>
        </xs:complexType>
    </xs:element>

    <!-- Catch-all record so that everything can always be parsed -->
    <xs:element name="TransactionRecordXX">
        <xs:complexType>
            <xs:sequence>
           		<xs:element dfdl:length="1" dfdl:lengthKind="explicit" name="StringType" type="sa:type_PS"/>
                <xs:element maxOccurs="unbounded" minOccurs="0" ref="sa:CustomUserField"/>
            </xs:sequence>
		</xs:complexType>
    </xs:element>
    
    <xs:element dfdl:emptyValueDelimiterPolicy="initiator" dfdl:encoding="ISO-8859-1" dfdl:initiator="%#x00;:" name="TransactionRecord00">
        <xs:complexType>
            <xs:sequence>
           		<!--
           		<xs:element dfdl:inputValueCalc="{xs:hexBinary('00')}" name="StringType" type="sa:type_PS">
           		</xs:element> 
           		-->
                <xs:element dfdl:length="2" dfdl:lengthKind="explicit" name="TerminalNumber" type="sa:type_PS">
                </xs:element>
                <xs:element dfdl:length="2" dfdl:lengthKind="explicit" ibmDfdlExtn:sampleValue="1" name="TransactionNumber" type="sa:type_PI">
                </xs:element>
                <xs:element dfdl:length="5" dfdl:lengthKind="explicit" name="DateTime" type="sa:type_PI">
                </xs:element>
                <xs:element dfdl:length="1" dfdl:lengthKind="explicit" ibmDfdlExtn:sampleValue="00" name="TransactionType" type="sa:type_PS">
                </xs:element>
                <xs:element minOccurs="0" name="NumberOfStrings" type="sa:type_PD">
                </xs:element>
                <xs:element minOccurs="0" name="Operator" type="sa:type_PD">
                </xs:element>
                <xs:element minOccurs="0" name="Password" type="sa:type_PD">
                </xs:element>
                <xs:element minOccurs="0" name="GrossPositive" type="sa:type_PL">
                </xs:element>
                <xs:element minOccurs="0" name="GrossNegative" type="sa:type_PL">
                </xs:element>
                <xs:element minOccurs="0" name="RingTime" type="sa:type_PL">
                </xs:element>
                <xs:element minOccurs="0" name="TenderTime" type="sa:type_PL">
                </xs:element>
                <xs:element minOccurs="0" name="SpecialTime" type="sa:type_PL">
                </xs:element>
                <xs:element minOccurs="0" name="InactiveTime" type="sa:type_PL">
                </xs:element>
                <!-- Descending bit order - not per SA PG -->
                <xs:element minOccurs="0" name="Indicat1" type="sa:type_4ByteBitField_Desc">
				</xs:element>
				<xs:element ibmDfdlExtn:sampleValue="0F00" maxOccurs="unbounded" minOccurs="0" ref="sa:CustomUserField"/>
            </xs:sequence>
		</xs:complexType>
    </xs:element>

    <xs:element dfdl:emptyValueDelimiterPolicy="initiator" dfdl:encoding="ISO-8859-1" dfdl:initiator="%#x01;:" name="TransactionRecord01">
	    <xs:complexType>
            <xs:sequence>
           		<!--
           		<xs:element dfdl:inputValueCalc="{xs:hexBinary('01')}" name="StringType" type="sa:type_PS"> 
           		-->
                <xs:element minOccurs="0" name="ItemCode" type="sa:type_PD">
                </xs:element>
                <xs:element minOccurs="0" name="ExtendedPrice" type="sa:type_PD">
                </xs:element>
                <xs:element minOccurs="0" name="DepartmentNumber" type="sa:type_PD">
                </xs:element>
                <xs:element minOccurs="0" name="CouponFamilyNumber" type="sa:type_PD">
                </xs:element>
                <!-- Reversed little endian bit order - not per SA PG -->
                <xs:element minOccurs="0" name="Indicat1" type="sa:type_2ByteBitField_ByteReversedLittleEndian">
                </xs:element>
                <!-- Reversed little endian bit order - not per SA PG -->
                <xs:element minOccurs="0" name="Indicat2" type="sa:type_2ByteBitField_ByteReversedLittleEndian">                                 
                </xs:element>
                <xs:element dfdl:occursCountKind="implicit" maxOccurs="1" minOccurs="0" name="Indicat3" type="sa:type_PS">
                </xs:element>
				<xs:element maxOccurs="unbounded" minOccurs="0" ref="sa:CustomUserField"/>
            </xs:sequence>
		</xs:complexType>
   	</xs:element>

    <xs:element dfdl:emptyValueDelimiterPolicy="initiator" dfdl:encoding="ISO-8859-1" dfdl:initiator="%#x02;:" name="TransactionRecord02">
	    <xs:complexType>
            <xs:sequence>
           		<!--
           		<xs:element dfdl:inputValueCalc="{xs:hexBinary('02')}" name="StringType" type="sa:type_PS"> 
           		-->
                <xs:element minOccurs="0" name="MultiPricingGroup" type="sa:type_PD">
                </xs:element>
                <xs:element minOccurs="0" name="DealQuantity" type="sa:type_PD">
                </xs:element>
                <xs:element minOccurs="0" name="PricingMethod" type="sa:type_PD">
                </xs:element>
                <xs:element minOccurs="0" name="SaleQuantity" type="sa:type_PD">
                </xs:element>
                <xs:element minOccurs="0" name="SalePrice" type="sa:type_PD">
                </xs:element>
                <xs:element minOccurs="0" name="QtyOrWgtOrVol" type="sa:type_PD">
                </xs:element>
                <!-- Assume ascending bit order per MRM and acespec.xml and not per SA PG -->
                <xs:element minOccurs="0" name="Indicat1" type="sa:type_1ByteBitField">
                </xs:element>
				<xs:element maxOccurs="unbounded" minOccurs="0" ref="sa:CustomUserField"/>
            </xs:sequence>
		</xs:complexType>
    </xs:element>

    <xs:element dfdl:emptyValueDelimiterPolicy="initiator" dfdl:encoding="ISO-8859-1" dfdl:initiator="%#x03;:" name="TransactionRecord03">
        <xs:complexType>
        	<xs:sequence>
           		<!--
           		<xs:element dfdl:inputValueCalc="{xs:hexBinary('03')}" name="StringType" type="sa:type_PS"> 
           		-->
            	<xs:group ref="sa:sequence_03_and_04">
            	</xs:group>
            </xs:sequence>
		</xs:complexType>
    </xs:element>

    <xs:element dfdl:emptyValueDelimiterPolicy="initiator" dfdl:encoding="ISO-8859-1" dfdl:initiator="%#x04;:" name="TransactionRecord04">
        <xs:complexType>
        	<xs:sequence>
           		<!--
           		<xs:element dfdl:inputValueCalc="{xs:hexBinary('04')}" name="StringType" type="sa:type_PS"> 
           		-->
    	        <xs:group ref="sa:sequence_03_and_04">
	            </xs:group>
            </xs:sequence>
		</xs:complexType>
    </xs:element>

	<xs:group name="sequence_03_and_04">
		<xs:sequence>
                <xs:element dfdl:length="1" dfdl:lengthKind="explicit" name="DiscountGroup" type="sa:type_PI">
                </xs:element>
                <xs:element minOccurs="0" name="DiscountRate" type="sa:type_PD">
                </xs:element>
                <xs:element minOccurs="0" name="DiscountAmount" type="sa:type_PL">
                </xs:element>
                <xs:element minOccurs="0" name="DiscountTaxExemptAmount" type="sa:type_PL">
                </xs:element>
				<xs:element maxOccurs="unbounded" minOccurs="0" ref="sa:CustomUserField"/>
		</xs:sequence>
	</xs:group>
	  
    <xs:element dfdl:emptyValueDelimiterPolicy="initiator" dfdl:encoding="ISO-8859-1" dfdl:initiator="%#x05;:" name="TransactionRecord05">
        <xs:complexType>
        	<xs:sequence>
           		<!--
           		<xs:element dfdl:inputValueCalc="{xs:hexBinary('05')}" name="StringType" type="sa:type_PS">
           		</xs:element> 
           		-->
	            <xs:group ref="sa:sequence_05_and_06">
    	        </xs:group>
            </xs:sequence>
		</xs:complexType>
    </xs:element>

    <xs:element dfdl:emptyValueDelimiterPolicy="initiator" dfdl:encoding="ISO-8859-1" dfdl:initiator="%#x06;:" name="TransactionRecord06">
        <xs:complexType>
        	<xs:sequence>
           		<!--
           		<xs:element dfdl:inputValueCalc="{xs:hexBinary('06')}" name="StringType" type="sa:type_PS">
           		</xs:element> 
           		-->
	            <xs:group ref="sa:sequence_05_and_06">
    	        </xs:group>
            </xs:sequence>
		</xs:complexType>
    </xs:element>

	<xs:group name="sequence_05_and_06">
		<xs:sequence>
                <xs:element dfdl:length="1" dfdl:lengthKind="explicit" name="TenderType" type="sa:type_PI">
                </xs:element>
                <xs:element minOccurs="0" name="TenderAmount" type="sa:type_PL">
                </xs:element>
                <xs:element minOccurs="0" name="TenderCashingFee" type="sa:type_PL">
                </xs:element>
                <xs:element minOccurs="0" name="Customer" type="sa:type_PD">
                </xs:element>
                <xs:element minOccurs="0" name="Status" type="sa:type_PI">
                </xs:element>
				<xs:element maxOccurs="unbounded" minOccurs="0" ref="sa:CustomUserField"/>
		</xs:sequence>
	</xs:group>

    <xs:element dfdl:emptyValueDelimiterPolicy="initiator" dfdl:encoding="ISO-8859-1" dfdl:initiator="%#x07;:" name="TransactionRecord07">
        <xs:complexType>
        	<xs:sequence>
           		<!--
           		<xs:element dfdl:inputValueCalc="{xs:hexBinary('07')}" name="StringType" type="sa:type_PS">
           		</xs:element> 
           		-->
	            <xs:group ref="sa:sequence_07_and_08">
    	        </xs:group>
            </xs:sequence>
		</xs:complexType>
    </xs:element>

    <xs:element dfdl:emptyValueDelimiterPolicy="initiator" dfdl:encoding="ISO-8859-1" dfdl:initiator="%#x08;:" name="TransactionRecord08">
        <xs:complexType>
        	<xs:sequence>
           		<!--
           		<xs:element dfdl:inputValueCalc="{xs:hexBinary('08')}" name="StringType" type="sa:type_PS">
           		</xs:element> 
           		-->
            	<xs:group ref="sa:sequence_07_and_08">
            	</xs:group>
            </xs:sequence>
		</xs:complexType>
    </xs:element>

	<xs:group name="sequence_07_and_08">
		<xs:sequence>
                <xs:element minOccurs="0" name="TaxAAmount" type="sa:type_PL">
                </xs:element>
                <xs:element minOccurs="0" name="TaxBAmount" type="sa:type_PL">
                </xs:element>
                <xs:element minOccurs="0" name="TaxCAmount" type="sa:type_PL">
                </xs:element>
                <xs:element minOccurs="0" name="TaxDAmount" type="sa:type_PL">
                </xs:element>
                <xs:element minOccurs="0" name="TaxASales" type="sa:type_PL">
                </xs:element>
                <xs:element minOccurs="0" name="TaxBSales" type="sa:type_PL">
                </xs:element>
                <xs:element minOccurs="0" name="TaxCSales" type="sa:type_PL">
                </xs:element>
                <xs:element minOccurs="0" name="TaxDSales" type="sa:type_PL">
                </xs:element>
				<xs:element maxOccurs="unbounded" minOccurs="0" ref="sa:CustomUserField"/>
		</xs:sequence>
	</xs:group>

    <xs:element dfdl:emptyValueDelimiterPolicy="initiator" dfdl:encoding="ISO-8859-1" dfdl:initiator="%#x09;:" name="TransactionRecord09">
        <xs:complexType>
            <xs:sequence>
           		<!--
           		<xs:element dfdl:inputValueCalc="{xs:hexBinary('09')}" name="StringType" type="sa:type_PS">
           		</xs:element> 
           		-->
                <xs:element dfdl:length="1" dfdl:lengthKind="explicit" name="TenderType" type="sa:type_PI">
                </xs:element>
                <xs:element minOccurs="0" name="TenderAmount" type="sa:type_PL">
                </xs:element>
				<xs:element maxOccurs="unbounded" minOccurs="0" ref="sa:CustomUserField"/>
            </xs:sequence>
		</xs:complexType>
    </xs:element>

    <xs:element dfdl:emptyValueDelimiterPolicy="initiator" dfdl:encoding="ISO-8859-1" dfdl:initiator="%#x10;:" name="TransactionRecord10">
        <xs:complexType>
            <xs:sequence>
           		<!--
           		<xs:element dfdl:inputValueCalc="{xs:hexBinary('10')}" name="StringType" type="sa:type_PS">
           		</xs:element> 
           		-->
                <xs:element minOccurs="0" name="Override" type="sa:type_PD">
                </xs:element>
                <xs:element minOccurs="0" name="Reason" type="sa:type_PS">
                </xs:element>
				<xs:element maxOccurs="unbounded" minOccurs="0" ref="sa:CustomUserField"/>
            </xs:sequence>
		</xs:complexType>
    </xs:element>

	<!-- Group for all 11 records --> 
	<!-- All 11 records discriminate on SubStringType except the DataEntry and catch-all records -->
	<!-- When dfdl:inputValueCalc supported, we will use an initiator instead -->
	<!-- User can add custom 11 records --> 
    <xs:group name="choice_11">
				<xs:choice dfdl:encoding="ISO-8859-1" dfdl:initiator="%#x11;:">
					<!-- All branches of the following choice must have the same length initiator -->
					<xs:choice>	
		                <xs:element ref="sa:TransactionRecord11_BB">
						</xs:element>
    		            <xs:element ref="sa:TransactionRecord11_CC">
						</xs:element>
    	        	    <xs:element ref="sa:TransactionRecord11_CE">
						</xs:element>
	    	            <xs:element ref="sa:TransactionRecord11_DB">
						</xs:element>
    	        	    <xs:element ref="sa:TransactionRecord11_DE">
						</xs:element>
	    	            <xs:element ref="sa:TransactionRecord11_EE">
						</xs:element>
    		            <xs:element ref="sa:TransactionRecord11_FF">
						</xs:element>
					</xs:choice>
					<!-- 11_DD has an extra unused field at start so breaks initiator length rule -->
   		            <xs:element ref="sa:TransactionRecord11_DD">
					</xs:element>
					<!-- Custom 11 records -->
			        <xs:group ref="sa:choice_11_Custom">
		            </xs:group>
    	            <xs:element ref="sa:TransactionRecord11_DataEntry">
					</xs:element>
    	            <xs:element ref="sa:TransactionRecord11_XX">
						<xs:annotation>
							<xs:appinfo source="http://www.ogf.org/dfdl/">
								<dfdl:discriminator>{fn:true()}</dfdl:discriminator>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
        	    </xs:choice>
    </xs:group>

	<!-- Data entry record for 11 -->
	<!-- Note this does not have a SubStringType field -->
    <xs:element name="TransactionRecord11_DataEntry">
        <xs:complexType>
            <xs:sequence>
           		<!--
           		<xs:element dfdl:inputValueCalc="{xs:hexBinary('11')}" name="StringType" type="sa:type_PS">
           		</xs:element> 
           		-->
                <xs:element minOccurs="0" name="UserData1" type="sa:type_PD">
                </xs:element>
                <xs:element minOccurs="0" name="UserData2" type="sa:type_PD">
                </xs:element>
                <xs:element minOccurs="0" name="UserData3" type="sa:type_PD">
                </xs:element>
                <xs:element minOccurs="0" name="UserData4" type="sa:type_PD">
                </xs:element>
                <xs:element minOccurs="0" name="UserData5" type="sa:type_PD">
                </xs:element>
                <xs:element minOccurs="0" name="UserData6" type="sa:type_PD">
                </xs:element>
                <xs:element maxOccurs="unbounded" minOccurs="0" ref="sa:CustomUserField"/>
            </xs:sequence>
		</xs:complexType>
    </xs:element>

	<!-- Catch-all record for 11 -->
	<!-- Note this does not have a SubStringType field -->
    <xs:element name="TransactionRecord11_XX">
        <xs:complexType>
            <xs:sequence>
           		<!--
           		<xs:element dfdl:inputValueCalc="{xs:hexBinary('11')}" name="StringType" type="sa:type_PS">
           		</xs:element> 
           		-->
                <xs:element maxOccurs="unbounded" minOccurs="0" ref="sa:CustomUserField"/>
            </xs:sequence>
		</xs:complexType>
    </xs:element>

    <xs:element name="TransactionRecord11_BB">
        <xs:complexType>
            <xs:sequence>
           		<!--
           		<xs:element dfdl:inputValueCalc="{xs:hexBinary('11')}" name="StringType" type="sa:type_PS">
           		</xs:element> 
           		-->
           	    <xs:element dfdl:length="1" dfdl:lengthKind="explicit" fixed="BB" name="SubStringType" type="sa:type_PS">
						<xs:annotation>
							<xs:appinfo source="http://www.ogf.org/dfdl/">
								<dfdl:discriminator>{. eq xs:hexBinary('BB')}</dfdl:discriminator>
							</xs:appinfo>
						</xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" name="AccountNumber" type="sa:type_PD">
                </xs:element>
                <xs:element minOccurs="0" name="Unused1" type="sa:type_PD">
                </xs:element>
                <xs:element minOccurs="0" name="Unused2" type="sa:type_PD">
                </xs:element>
                <xs:element minOccurs="0" name="Unused3" type="sa:type_PD">
                </xs:element>
                <xs:element minOccurs="0" name="Unused4" type="sa:type_PD">
                </xs:element>
                <xs:element minOccurs="0" name="Unused5" type="sa:type_PD">
                </xs:element>
                <xs:element maxOccurs="unbounded" minOccurs="0" ref="sa:CustomUserField"/>
            </xs:sequence>
		</xs:complexType>
    </xs:element>

    <xs:element name="TransactionRecord11_CC">
        <xs:complexType>
            <xs:sequence>
           		<!--
           		<xs:element dfdl:inputValueCalc="{xs:hexBinary('11')}" name="StringType" type="sa:type_PS">
           		</xs:element> 
           		-->
           	    <xs:element dfdl:length="1" dfdl:lengthKind="explicit" fixed="CC" name="SubStringType" type="sa:type_PS">
						<xs:annotation>
							<xs:appinfo source="http://www.ogf.org/dfdl/">
								<dfdl:discriminator>{. eq xs:hexBinary('CC')}</dfdl:discriminator>
							</xs:appinfo>
						</xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" name="ManufacturerCouponAmount" type="sa:type_PD">
                </xs:element>
                <xs:element minOccurs="0" name="StoreCouponAmount" type="sa:type_PD">
                </xs:element>
